{
    "name": "copyplaintext",
    "displayName": "CopyPlainText",
    "description": "Copy plain text with no formatting.",
    "version": "1.0.0",
    "publisher": "p26frqxxfmwrzekd6c6fcty5kipjkr3gaeejzpkyy42p5udjkjyq",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyPlainText"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.copyPlainText",
            "title": "Copy Plain Text"
        }],
        "menus": {
            "commandPalette": [{
                "command": "extension.copyPlainText",
                "when": "editorHasSelection"
            }],
            "editor/context": [{
                "when": "editorHasSelection",
                "command": "extension.copyPlainText",
                "group": "9_cutcopypaste@2"
            }]
        },
        "keybindings": [{
            "command": "extension.copyPlainText",
            "key": "ctrl+alt+c",
            "when": "editorHasSelection"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    },
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/xan1000/copyplaintext/src/master/"
    },
    "homepage": "https://bitbucket.org/xan1000/copyplaintext"
}